<% provide(:title, 'Create new Project') %>
<h1>New Project</h1>
    <%= form_for(@project) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_field :description %>

     
      <%= f.submit "Create Project" %>
    <% end %>
